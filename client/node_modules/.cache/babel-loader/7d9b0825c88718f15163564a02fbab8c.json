AA,CAAA;AAPA,iBAFA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAcA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA;;AACA,gBAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA;;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA,EAhBA,CAkBA;;;AAlBA;AAAA;;AAAA;AAAA;AAAA;AAoBA,gBAAA,OAAA,CAAA,GAAA,CAAA,YAAA,QAAA;AACA,gBAAA,KAAA,CAAA,IAAA,CAAA,WAAA,GAAA,YAAA,QAAA,CAAA,IAAA,CAAA,OAAA;;AArBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuBA,KA5BA;AA6BA,IAAA,UA7BA,wBA6BA;AACA;AACA,WAAA,IAAA,CAAA,WAAA,GAAA,SAAA,CAFA,CAEA;;AACA,WAAA,EAAA,CAAA,IAAA,CAAA,MAAA;;AACA,UAAA,KAAA,EAAA,CAAA,IAAA,CAAA,SAAA,EAAA;AACA;AACA,OANA,CAOA;;;AACA,WAAA,QAAA;AACA,KAtCA;AAwCA,IAAA,OAxCA,qBAwCA;AAAA;;AACA,WAAA,IAAA,GAAA;AACA,QAAA,QAAA,EAAA,EADA;AAEA,QAAA,SAAA,EAAA,EAFA;AAGA,QAAA,QAAA,EAAA,EAHA;AAIA,QAAA,OAAA,EAAA,IAJA;AAKA,QAAA,QAAA,EAAA,EALA;AAMA,QAAA,eAAA,EAAA,EANA;AAOA,QAAA,KAAA,EAAA,EAPA;AAQA,QAAA,OAAA,EAAA;AARA,OAAA;AAUA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,EAAA,CAAA,MAAA;AACA,OAFA;AAGA;AAtDA;AAxDA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <h1 class=\"title\">Register</h1>\n    <b-form @submit.prevent=\"onRegister\" @reset.prevent=\"onReset\">\n      <b-form-group\n        id=\"input-group-username\"\n        label-cols-sm=\"3\"\n        label=\"Username:\"\n        label-for=\"username\"\n      >\n        <b-form-input\n          id=\"username\"\n          v-model=\"$v.form.username.$model\"\n          type=\"text\"\n          :state=\"validateState('username')\"\n        ></b-form-input>\n        <b-form-invalid-feedback v-if=\"!$v.form.username.required\">\n          Username is required\n        </b-form-invalid-feedback>\n        <b-form-invalid-feedback v-else-if=\"!$v.form.username.length\">\n          Username length should be between 3-8 characters long\n        </b-form-invalid-feedback>\n        <b-form-invalid-feedback v-if=\"!$v.form.username.alpha\">\n          Username alpha\n        </b-form-invalid-feedback>\n      </b-form-group>\n\n      <b-form-group\n        id=\"input-group-firstName\"\n        label-cols-sm=\"3\"\n        label=\"FirstName:\"\n        label-for=\"firstName\"\n      >\n        <b-form-input\n          id=\"firstName\"\n          v-model=\"$v.form.firstName.$model\"\n          type=\"text\"\n          :state=\"validateState('firstName')\"\n        ></b-form-input>\n        <b-form-invalid-feedback v-if=\"!$v.form.username.required\">\n          first name is required\n        </b-form-invalid-feedback>\n\n        <b-form-invalid-feedback v-if=\"!$v.form.username.alpha\">\n          first name alpha\n        </b-form-invalid-feedback>\n      </b-form-group>\n\n      <b-form-group\n        id=\"input-group-lastName\"\n        label-cols-sm=\"3\"\n        label=\"LastName:\"\n        label-for=\"lastName\"\n      >\n        <b-form-input\n          id=\"lastName\"\n          v-model=\"$v.form.lastName.$model\"\n          type=\"text\"\n          :state=\"validateState('lastName')\"\n        ></b-form-input>\n        <b-form-invalid-feedback v-if=\"!$v.form.username.required\">\n          last name is required\n        </b-form-invalid-feedback>\n\n        <b-form-invalid-feedback v-if=\"!$v.form.username.alpha\">\n          last name alpha\n        </b-form-invalid-feedback>\n      </b-form-group>\n\n      <b-form-group\n        id=\"input-group-country\"\n        label-cols-sm=\"3\"\n        label=\"Country:\"\n        label-for=\"country\"\n      >\n        <b-form-select\n          id=\"country\"\n          v-model=\"$v.form.country.$model\"\n          :options=\"countries\"\n          :state=\"validateState('country')\"\n        ></b-form-select>\n        <b-form-invalid-feedback>\n          Country is required\n        </b-form-invalid-feedback>\n      </b-form-group>\n\n      <b-form-group\n        id=\"input-group-Password\"\n        label-cols-sm=\"3\"\n        label=\"Password:\"\n        label-for=\"password\"\n      >\n        <b-form-input\n          id=\"password\"\n          type=\"password\"\n          v-model=\"$v.form.password.$model\"\n          :state=\"validateState('password')\"\n        ></b-form-input>\n        <b-form-invalid-feedback v-if=\"!$v.form.password.required\">\n          Password is required\n        </b-form-invalid-feedback>\n        <b-form-text v-else-if=\"$v.form.password.$error\" text-variant=\"info\">\n          Your password should be <strong>strong</strong>. <br />\n          For that, your password should be also:\n        </b-form-text>\n        <b-form-invalid-feedback\n          v-if=\"$v.form.password.required && !$v.form.password.length\"\n        >\n          Have length between 5-10 characters long\n        </b-form-invalid-feedback>\n      </b-form-group>\n\n      <b-form-group\n        id=\"input-group-confirmPassword\"\n        label-cols-sm=\"3\"\n        label=\"Confirm Password:\"\n        label-for=\"confirmPassword\"\n      >\n        <b-form-input\n          id=\"confirmPassword\"\n          type=\"password\"\n          v-model=\"$v.form.confirmPassword.$model\"\n          :state=\"validateState('confirmPassword')\"\n        ></b-form-input>\n        <b-form-invalid-feedback v-if=\"!$v.form.confirmPassword.required\">\n          Password confirmation is required\n        </b-form-invalid-feedback>\n        <b-form-invalid-feedback\n          v-else-if=\"!$v.form.confirmPassword.sameAsPassword\"\n        >\n          The confirmed password is not equal to the original password\n        </b-form-invalid-feedback>\n      </b-form-group>\n\n      <b-form-group\n        id=\"input-group-email\"\n        label-cols-sm=\"3\"\n        label=\"email:\"\n        label-for=\"email\"\n      >\n        <b-form-input\n          id=\"email\"\n          v-model=\"$v.form.email.$model\"\n          type=\"text\"\n          :state=\"validateState('email')\"\n        ></b-form-input>\n        <b-form-invalid-feedback v-if=\"!$v.form.username.required\">\n          email is required\n        </b-form-invalid-feedback>\n\n        <b-form-invalid-feedback v-if=\"!$v.form.username.email\">\n          email format is required\n        </b-form-invalid-feedback>\n      </b-form-group>\n\n      <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\n      <b-button\n        type=\"submit\"\n        variant=\"primary\"\n        style=\"width:250px;\"\n        class=\"ml-5 w-75\"\n        >Register</b-button\n      >\n      <div class=\"mt-2\">\n        You have an account already?\n        <router-link to=\"login\"> Log in here</router-link>\n      </div>\n    </b-form>\n    <b-alert\n      class=\"mt-2\"\n      v-if=\"form.submitError\"\n      variant=\"warning\"\n      dismissible\n      show\n    >\n      Register failed: {{ form.submitError }}\n    </b-alert>\n    <!-- <b-card class=\"mt-3 md-3\" header=\"Form Data Result\">\n      <pre class=\"m-0\"><strong>form:</strong> {{ form }}</pre>\n      <pre class=\"m-0\"><strong>$v.form:</strong> {{ $v.form }}</pre>\n    </b-card> -->\n  </div>\n</template>\n\n<script>\nimport countries from \"../assets/countries\";\nimport {\n  required,\n  minLength,\n  maxLength,\n  alpha,\n  sameAs,\n  email,\n} from \"vuelidate/lib/validators\";\n\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      form: {\n        username: \"\",\n        firstName: \"\",\n        lastName: \"\",\n        country: null,\n        password: \"\",\n        confirmPassword: \"\",\n        email: \"\",\n        submitError: undefined,\n      },\n      countries: [{ value: null, text: \"\", disabled: true }],\n      errors: [],\n      validated: false,\n    };\n  },\n  validations: {\n    form: {\n      username: {\n        required,\n        length: (u) => minLength(3)(u) && maxLength(8)(u),\n        alpha,\n      },\n      country: {\n        required,\n      },\n      password: {\n        required,\n        length: (p) => minLength(5)(p) && maxLength(10)(p),\n      },\n      confirmPassword: {\n        required,\n        sameAsPassword: sameAs(\"password\"),\n      },\n      firstName: {\n        required,\n        alpha,\n      },\n      lastName: {\n        required,\n        alpha,\n      },\n      email: {\n        required,\n        email,\n      },\n    },\n  },\n  mounted() {\n    // console.log(\"mounted\");\n    this.countries.push(...countries);\n    // console.log($v);\n  },\n  methods: {\n    validateState(param) {\n      const { $dirty, $error } = this.$v.form[param];\n      return $dirty ? !$error : null;\n    },\n    async Register() {\n      try {\n        const response = await this.axios.post(\n          \"http://localhost:3001/register\",\n          {\n            username: this.form.username,\n            password: this.form.password,\n            country: this.form.country,\n            firstName: this.form.firstName,\n            lastName: this.form.lastName,\n            email: this.form.email,\n            confirmPassword: this.form.confirmPassword,\n          }\n        );\n        console.log(this.$root.store.login);\n        this.$root.store.login(this.form.username);\n        this.$router.push(\"/login\");\n\n        // console.log(response);\n      } catch (err) {\n        console.log(err.response);\n        this.form.submitError = err.response.data.message;\n      }\n    },\n    onRegister() {\n      // console.log(\"register method called\");\n      this.form.submitError = undefined; //maybe\n      this.$v.form.$touch();\n      if (this.$v.form.$anyError) {\n        return;\n      }\n      // console.log(\"register method go\");\n      this.Register();\n    },\n\n    onReset() {\n      this.form = {\n        username: \"\",\n        firstName: \"\",\n        lastName: \"\",\n        country: null,\n        password: \"\",\n        confirmPassword: \"\",\n        email: \"\",\n        picture: null,\n      };\n      this.$nextTick(() => {\n        this.$v.$reset();\n      });\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.container {\n  max-width: 500px;\n}\n</style>\n"],"sourceRoot":"src/pages"}]}